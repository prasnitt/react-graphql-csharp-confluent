{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Producer": {
    "BootstrapServers": "pkc-921jm.us-east-2.aws.confluent.cloud:9092",
    "ClientId": "MessageProducer",
    "SecurityProtocol": "SaslSsl",
    "SaslMechanism": "PLAIN",
    "SaslUsername": "<Confluent-API-Key>",
    "SaslPassword": "<Confluent-API-Key Secret>",
    "EnableIdempotence": true
  },

  "SchemaRegistry": {
    "URL": "https://psrc-k6gvm.us-west-2.aws.confluent.cloud",
    "BasicAuthCredentialsSource": "UserInfo",
    "BasicAuthUserInfo": "<Confluent-Schema-Registry-API-Key>:<Confluent-Schema-Registry-API-Key-Secret>"
  }
}
